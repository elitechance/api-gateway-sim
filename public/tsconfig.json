{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [ "es2015", "dom" ],
    "noImplicitAny": true,
    "rootDir":"./src/",
    "outDir":"../dist/public",
    "suppressImplicitAnyIndexErrors": true
  },
  "include":["./src/**/*"],
  "exclude": [ "node_modules"]
}
